page {
    height: 100%
}
.container {
    height: 100%
}
.title {
    margin-top: 300rpx;
    font-size: 20px;
    color: rgb(255,255,255);
    text-shadow: 5rpx 5rpx 5rpx red;
    text-stroke: 5rpx black
}
scroll-view {
    position: absolute;
    top: 400rpx;
    overflow: hidden
}
.tab {
    background: #E6E6FA;
    width: 100%;
    height: 100px;
    border-radius: 0 10px 0 10px;
    text-align: center;
    line-height: 100px
}
.tab + .tab {
    margin-top: 5px
}